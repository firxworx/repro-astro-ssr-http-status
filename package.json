{
  "name": "astro-ssr-wrong-http-status-repro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "pnpm typecheck && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "typecheck": "astro check",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "db:generate": "drizzle-kit generate --config=drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config=drizzle.config.ts",
    "db:push": "drizzle-kit push --config=drizzle.config.ts",
    "db:seed": "tsx data/scripts/seed.ts",
    "auth:generate": "cross-env ENV_FILE=.env pnpm dlx @better-auth/cli@latest generate --config src/lib/server/auth.ts",
    "auth:secret": "pnpm dlx @better-auth/cli@latest secret"
  },
  "dependencies": {
    "@astrojs/node": "^9.0.1",
    "@astrojs/react": "^4.1.5",
    "astro": "^5.1.7",
    "better-auth": "^1.1.14",
    "better-sqlite3": "^11.8.0",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.38.4",
    "i18next": "^24.2.1",
    "i18next-browser-languagedetector": "^8.0.2",
    "i18next-fs-backend": "^2.6.0",
    "i18next-http-backend": "^3.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^5.0.0",
    "react-i18next": "^15.4.0",
    "ufo": "^1.5.4",
    "vite": "^6.0.7"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@biomejs/biome": "^1.9.4",
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^22.10.7",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.30.2",
    "tsx": "^4.19.2"
  }
}
