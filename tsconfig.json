{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "strict": true,
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "exactOptionalPropertyTypes": false,
    "types": ["@types/react", "@types/react-dom"]
  },
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"]
}
